{
    "name": "spatie/laravel-varnish",
    "description": "Making Varnish and Laravel play nice together",
    "keywords": [
        "spatie",
        "laravel-varnish"
    ],
    "homepage": "https://github.com/spatie/laravel-varnish",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "~5.5.0|~5.6.0",
        "illuminate/http": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Varnish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Varnish\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Varnish\\VarnishServiceProvider"
            ]
        }
    }
}
